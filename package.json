{
    "name": "@slimio/npm-registry",
    "version": "0.5.0",
    "description": "Node.js API to consume npm Registry",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/npm-registry.git"
    },
    "keywords": [
        "SlimIO",
        "npm",
        "registry",
        "node api"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/npm-registry/issues"
    },
    "homepage": "https://github.com/SlimIO/npm-registry#readme",
    "dependencies": {
        "@slimio/is": "^1.5.1",
        "httpie": "^1.1.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "ava": "^2.1.0",
        "dotenv": "^8.0.0",
        "eslint": "^6.0.0",
        "husky": "^2.4.1",
        "jsdoc": "^3.6.2",
        "nyc": "^14.0.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
